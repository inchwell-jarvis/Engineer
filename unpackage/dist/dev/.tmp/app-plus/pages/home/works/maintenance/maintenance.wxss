.box {
  overflow: auto;
}
.box .basicinformation {
    width: 95%;
    background: #FFFFFF;
    margin: auto;
    border-radius: 10px;
    margin-top: 10px;
}
.box .basicinformation .Task1 {
      width: 100%;
      padding: 10px;
      box-sizing: border-box;
}
.box .basicinformation .Task1 .head {
        width: 80%;
        height: 25px;
        background-color: #007AFF;
        color: #FFFFFF;
        border-radius: 5px;
        font-weight: bold;
        overflow: hidden;
        line-height: 25px;
        padding-left: 15px;
        box-sizing: border-box;
        font-size: 13px;
}
.box .basicinformation .Task1 .ps {
        width: 40%;
        margin-left: 60%;
        height: 30px;
        line-height: 30px;
}
.box .basicinformation .Task1 .ps image {
          display: block;
          width: 25px;
          height: 25px;
          float: left;
          margin-top: 3px;
}
.box .basicinformation .Task1 .EquPhotos {
        width: 100%;
        height: 135px;
        background-color: #FFFFFF;
}
.box .basicinformation .Task1 .EquPhotos .texts {
          width: 100%;
          height: 20px;
          line-height: 20px;
          font-size: 13px;
}
.box .basicinformation .Task1 .EquPhotos .Photos1s {
          margin-left: 60px;
}
.box .basicinformation .Task1 .EquPhotos .Photos1 {
          width: 35%;
          height: 110px;
          overflow: hidden;
          float: left;
}
.box .basicinformation .Task1 .EquPhotos .Photos1 image {
            width: 100%;
            height: 100%;
}
.box .basicinformation .Task1 .EquPhotos .Photos1 .box {
            overflow: hidden;
            background: #FFFFFF;
}
.box .basicinformation .text {
      width: 95%;
      height: 30px;
      display: block;
      margin: auto;
      line-height: 30px;
}
.box .basicinformation .text .span {
        width: 100px;
        display: block;
        height: 100%;
        float: left;
}
.box .cu-form-groupas {
    height: 40px;
    padding: 0;
}
.box .cu-form-group {
    width: 95%;
    background: #FFFFFF;
    border: 1px solid #ccc;
    box-sizing: border-box;
    padding: 10px;
    color: #999999;
    margin: auto;
    border-radius: 10px;
    margin-top: 10px;
}
.box .cu-form-group .title {
      width: 100px;
      display: block;
      float: left;
      color: #333333;
}
.box .cu-form-group input {
      font-size: 14px;
}
.box .align-start .title {
    font-weight: bold;
}
.box .align-start uni-textarea {
    margin-top: 20px;
}
.box .button {
    width: 80%;
    display: block;
    margin: auto;
    background-color: #007AFF;
    color: #FFFFFF;
    height: 50px;
    text-align: center;
    line-height: 50px;
    margin-top: 50px;
    border-radius: 10px;
    box-sizing: border-box;
    font-size: 20px;
    margin-bottom: 50px;
}
.box .buttons {
    width: 80%;
    display: block;
    margin: auto;
    color: #FFFFFF;
    height: 50px;
    text-align: center;
    line-height: 50px;
    margin-top: 50px;
    box-sizing: border-box;
    margin-bottom: 50px;
}
.box .buttons .jie {
      width: 40%;
      height: 80%;
      float: right;
      background-color: #007AFF;
      line-height: 40px;
      border-radius: 5px;
}
.box .buttons .jujue {
      width: 40%;
      height: 80%;
      float: left;
      background-color: #FF0000;
      line-height: 40px;
      border-radius: 5px;
}
.box .bottomone {
    width: 100%;
    height: 250px;
    background-color: #FFFFFF;
}
.box .bottomone .topone {
      width: 100%;
      height: 30px;
      padding: 10px;
}
.box .bottomone .topone button {
        float: right;
        margin-right: 30px;
}
.box .bottomone .bomone {
      width: 100%;
      height: calc(100% - 30px);
      overflow: auto;
}
.box .bottomone .bomone .kuang {
        width: 50%;
        height: 30px;
        margin-left: 20%;
        margin-top: 3px;
}
.box .bottomone .bomone .kuang switch {
          float: right;
}
.box .uni-textarea {
    width: 95%;
    border-radius: 10px;
    margin: auto;
    height: 200px;
    margin-top: 10px;
    background-color: #FFFFFF;
    overflow: auto;
    padding: 5px;
    box-sizing: border-box;
    font-size: 15px;
}

